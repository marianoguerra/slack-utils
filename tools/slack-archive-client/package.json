{
  "name": "slack-archive-client",
  "version": "0.1.0",
  "description": "JavaScript client library for slack-archive-server",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --format esm --target browser && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "dist": "bun run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@duckdb/duckdb-wasm": "^1.32.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": ">=1.32.0"
  },
  "peerDependenciesMeta": {
    "@duckdb/duckdb-wasm": {
      "optional": true
    }
  },
  "keywords": [
    "slack",
    "archive",
    "client",
    "parquet"
  ],
  "license": "MIT"
}
